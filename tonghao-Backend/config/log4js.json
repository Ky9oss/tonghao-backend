{
  "appenders": {
    "console": {
      "type": "console"
    },
    "file": {
      "type": "file",
      "filename": "./log/app.log",
      "maxLogSize": 10485760,
      "backups": 5,
      "compress": true
    }
  },
  "categories": {
    "default": {
      "appenders": ["console", "file"],
      "level": "info"
    }
  }
}
